POST http://localhost:4000/graphql
Content-Type: application/json
X-REQUEST-TYPE: GraphQL
# Authorization: Bearer

query GetAllCategories{
  categories{
    id
    code
    name
    description
    items{
      id
      name
      description
      value
      imagePath
      categories{
        code
        name
      }
    }
  }
}

